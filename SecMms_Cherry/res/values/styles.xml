<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="TextAppearance.SearchResult" parent="@style/TextAppearance">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimaryInverse</item>
        <item name="android:textColorHint">?android:textColorHintInverse</item>
    </style>
    <style name="TextAppearance.SearchResult.Title" parent="@style/TextAppearance.SearchResult">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="TextAppearance.SearchResult.Subtitle" parent="@style/TextAppearance.SearchResult">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">?android:textColorSecondaryInverse</item>
    </style>
    <style name="MmsHoloTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar" />
    <style name="locationActionBar" parent="@style/Theme.DeviceDefault">
        <item name="android:windowActionBarOverlay">false</item>
    </style>
    <style name="templateStyle">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Theme.Translucent" parent="@style/Theme.DeviceDefault">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="WidgetTitle">
        <item name="android:textSize">19.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#fff0f0f0</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="NotificationPrimaryText">
        <item name="android:textColor">#ffcccccc</item>
    </style>
    <style name="NotificationSecondaryText">
        <item name="android:textColor">#ff666666</item>
    </style>
    <style name="NotificationSubjectText">
        <item name="android:textColor">#ff999999</item>
    </style>
    <style name="Theme.DeviceDefault" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@style/MsgActionBar</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionbarIconDisable</item>
    </style>
    <style name="MsgActionBar" parent="@android:style/Widget.DeviceDefault.Light.ActionBar">
        <item name="android:displayOptions">showTitle</item>
    </style>
    <style name="ActionBarTitle" parent="@android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Title.Inverse" />
    <style name="ActionBarSubtitle" parent="@android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Subtitle.Inverse" />
    <style name="Theme.QuickContact" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="WindowNoTitle">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="TwHelpDialogTheme" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="NoAnimationDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="NoAnimation" parent="@android:style/Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>
    <style name="ActionbarIconDisable" parent="@android:style/Widget.DeviceDefault.ActionButton.CloseMode">
        <item name="android:visibility">gone</item>
    </style>
    <style name="QuickReplyDialogTheme" parent="@style/Theme.DeviceDefault">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="NoTitleTheme" parent="@style/Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="TextAppearanceNormal">
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="PopupItemDropDownStyle" parent="@android:attr/dropDownItemStyle">
        <item name="android:textSize">20.0dip</item>
        <item name="android:textColor">@color/popup_item_text_color</item>
    </style>
    <style name="ScoverTheme" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowDisablePreview">true</item>
    </style>
</resources>
